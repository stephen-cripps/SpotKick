<Window x:Class="SpotKick.Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpotKick.Desktop"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300
		">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
			<ColumnDefinition Width="300"/>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition></RowDefinition>
			<RowDefinition></RowDefinition>
			<RowDefinition></RowDefinition>
		</Grid.RowDefinitions>
<TextBox x:Name="SongKickUsername" Text="stephen.cripps" TextWrapping="NoWrap"
		         Grid.Row="0" Grid.Column="1" />
		<Button x:Name="Run" Content="Run" Click="Button_Click"
		        Grid.Row="1" Grid.Column="1" />
	</Grid>
</Window>
